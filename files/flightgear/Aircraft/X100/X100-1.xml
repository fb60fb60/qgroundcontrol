            <location unit="M">
                <x> 0.27 </x>
                <y> 0    </y>
                <z> 0    </z>
            </location>
            <orient unit="DEG">
                <roll> 0.0 </roll>
                <pitch> 0 </pitch>
                <yaw> 0 </yaw>
            </orient>
            <feed>0</feed>
